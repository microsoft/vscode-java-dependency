      "explorer/context": [
        {
          "submenu": "javaProject.newJavaFile",
          "when": "explorerResourceIsFolder",
          "group": "1_javaactions@10"
        },
        {
          "command": "java.view.fileExplorer.newPackage",
          "when": "explorerResourceIsFolder",
          "group": "1_javaactions@20"
