          "group": "7_modification@10"
        },
        {
          "command": "java.view.package.renameFile",
          "when": "view == javaProjectExplorer && viewItem =~ /java:(file|type|folder)(?=.*?\\b\\+uri\\b)/",
          "group": "7_modification@10"
